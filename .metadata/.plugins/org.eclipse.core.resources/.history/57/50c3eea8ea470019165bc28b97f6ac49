package com.yychatserver.controller;

import java.io.IOException;
import java.net.ServerSocket;

public class StartServer {
	ServerSocket ss;
	public StartServer(){
	 try {
		ss=new ServerSocket(3456);
	System.out.println("服务器已经启动，监听3456端口");
	ss.accept();
} catch (IOException e) {
		e.printStackTrace();
	}
	}
}
